{
  "name": "express-ts-app-starter-template",
  "version": "1.0.0",
  "description": "Microservice starter template.",
  "main": "server.ts",
  "scripts": {
    "clean": "rimraf build/*",
    "build": "npm run clean && npx tsc",
    "start": "node build/server.js",
    "postinstall": "tsc",
    "watch-node": "nodemon build/server.js",
    "watch-ts": "tsc -w",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q build/server.js\"",
    "test": "jest --runInBand --force-exit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mamun-swe/express-ts-app-starter-template"
  },
  "keywords": [
    "RESTAPI",
    "Microservice",
    "Nodejs",
    "TypeScript",
    "MongoDB",
    "Mongoose"
  ],
  "author": "Ride Care Ltd.",
  "license": "ISC",
  "dependencies": {
    "async-validator": "^4.2.5",
    "axios": "^1.1.2",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "mongoose": "^6.6.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/helmet": "^4.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "async-validator": "^4.2.5",
    "concurrently": "^7.4.0",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "bugs": {
    "url": "https://github.com/Mamun-swe/express-ts-app-starter-template/issues"
  },
  "homepage": "https://github.com/Mamun-swe/express-ts-app-starter-template#readme",
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  }
}
